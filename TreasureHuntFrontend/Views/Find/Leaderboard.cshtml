@model IEnumerable<Functions.Models.Find>

@{
    ViewData["Title"] = "Leaderboard";
}

<h1>Leaderboard</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FinderName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FinderName)
            </td>
        </tr>
}
    </tbody>
</table>
